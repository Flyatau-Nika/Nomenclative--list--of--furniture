<Window x:Class="Nomenclative__list__of__furniture.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Nomenclative__list__of__furniture"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Grid.Background>
            <ImageBrush ImageSource="Background/d2b967785cfbddf1fa0b3fd4a0253e4e.jpg">
            </ImageBrush>
        </Grid.Background>
        <TextBox x:Name="passwordBox" HorizontalAlignment="Left" Height="23" Margin="196,150,0,0" TextWrapping="Wrap" Text="Введите пароль" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="enter" Content="Войти" HorizontalAlignment="Left" Margin="214,265,0,0" VerticalAlignment="Top" Width="75" Click="enter_Click"/>
        <Button x:Name="savepassword" Content="Сохранить пароль" HorizontalAlignment="Left" Margin="196,198,0,0" VerticalAlignment="Top" Width="120" Click="savepassword_Click"/>
        <Label x:Name="label" Content="You are welcome!" HorizontalAlignment="Left" Margin="68,10,0,0" VerticalAlignment="Top" FontSize="48"/>

    </Grid>
</Window>
